<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.lectomundo.controller.admin.AdminMembresiaControlador"
            prefHeight="600.0" prefWidth="1000.0">

    <center>

        <TableView fx:id="tblMembresias" prefHeight="550" style="-fx-background-color: #212f3c; -fx-padding: 15;">

            <columns>

                <TableColumn fx:id="colId" text="ID" prefWidth="50"/>
                <TableColumn fx:id="colIdCliente" text="Id de Usuario" prefWidth="130"/>
                <TableColumn fx:id="colFechaInicio" text="Fecha de Inicio" prefWidth="140"/>
                <TableColumn fx:id="colFechaFin" text="Fecha de Fin" prefWidth="140"/>
                <TableColumn fx:id="colCosto" text="Costo" prefWidth="120"/>

            </columns>

        </TableView>

    </center>

</BorderPane>
